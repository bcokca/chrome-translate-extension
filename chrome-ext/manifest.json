{
  "manifest_version": 2,
  "name": "Selection Translator",
  "short_name":"Selection Translator",
  "description": "This extension helps to translate the selected text.",
  "version": "0.0.1",
  "icons":
  {
  	"128":"icon.png",
  	"16":"icon.png",
  	"48":"icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Selection Translator"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
